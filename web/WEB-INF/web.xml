    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageLoginLogOut</servlet-name>
        <url-pattern>/ManageLoginLogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RstPass</servlet-name>
        <url-pattern>/RstPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegiAdmin</servlet-name>
        <url-pattern>/RegiAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageMoreData</servlet-name>
