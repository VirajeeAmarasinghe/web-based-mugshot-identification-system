        <servlet-name>ManageLoginLogOut</servlet-name>
        <servlet-class>servltes.ManageLoginLogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RstPass</servlet-name>
        <servlet-class>servltes.RstPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegiAdmin</servlet-name>
        <servlet-class>servltes.RegiAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageMoreData</servlet-name>
        <servlet-class>servltes.ManageMoreData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageCrimeType</servlet-name>
        <servlet-class>servltes.ManageCrimeType</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManageCriminal</servlet-name>
        <url-pattern>/ManageCriminal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageCriminalImageUpload</servlet-name>
        <url-pattern>/ManageCriminalImageUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageSearchImage</servlet-name>
        <url-pattern>/ManageSearchImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageCrime</servlet-name>
