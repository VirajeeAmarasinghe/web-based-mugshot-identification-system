    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageCrimeType</servlet-name>
        <url-pattern>/ManageCrimeType</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
